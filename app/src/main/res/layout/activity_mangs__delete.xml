<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mangs_Delete">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="40dp"
    android:layout_marginRight="20dp"
    tools:ignore="MissingConstraints">

    <Button
        android:id="@+id/del_cat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deletCatogrey" />

    <Button
        android:id="@+id/del_pro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deletProduct" />

    <Button
        android:id="@+id/del_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deleteUser"
        />
    <Button
        android:id="@+id/del_cart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deletecart"
        />

    <Button
        android:id="@+id/del_report"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deleteReport"
        />

</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>